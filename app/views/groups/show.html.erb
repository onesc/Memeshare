<p>
Group Name:  <b> <%= @group.name %> </b> </br>
Join Code:  <%= @group.join_code %>
</p>
<p>
  <h2>Members </h2>
  <%= @group_members %>
</p>

<%= link_to("Leave Group",leave_group_path(@group), :method => :delete) %>

<% @images.reverse.each do |i| %>
<li>
   <%= link_to "View Image", i %><img src = <%=i.image.url%>> </br>
Uploaded on: <%=  i.created_at %> by <%= User.find(i.user_id).name %>
</br>

<hr>
</li>
<% end %>
